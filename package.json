  "scripts": {
    "start": "node develop.js",
    "compile": "webpack-command --progress --mode production",
    "compile-profile": "webpack-command --mode production --reporter json > stats.json",
    "test": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json mocha -r ts-node/register -r tsconfig-paths/register ./src/**/*.spec.ts",
    "single-test": "mocha -r ts-node/register -r tsconfig-paths/register",
    "integration-tests": "node release/ci-integration-tests.js",
    "upload-coverage": "codecov -f ./coverage/lcov.info",
    "i18n": "node release/ci-i18n-check.js",
    "ts-check": "tsc --noEmit --skipLibCheck",
    "react-lint": "react-lint ./src/renderer/index.tsx"
